{
  "compilerOptions": {
    "esModuleInterop": true,
    "incremental": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "strict": true,
    "noImplicitReturns": true,
    "moduleResolution": "node",
    "stripInternal": true,
    "noEmit": true,
    "lib": [
      "esnext",
      "dom"
    ],
    "target": "esnext",
    "baseUrl": ".",
    "paths": {
      "@vinrobot/vmware-content-library": [
        "./src/index"
      ]
    }
  }
}
