{
  "name": "scraper",
  "version": "1.0.0",
  "description": "",
  "author": "Vinrobot",
  "license": "MIT",
  "bin": {
    "scraper": "./bin/run"
  },
  "homepage": "https://github.com/Vinrobot/ubuntu-cloud-images",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vinrobot/ubuntu-cloud-images.git"
  },
  "bugs": {
    "url": "https://github.com/Vinrobot/ubuntu-cloud-images/issues"
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "version": "oclif readme && git add README.md"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^2.8.5",
    "@oclif/plugin-help": "^5.2.9",
    "@oclif/plugin-plugins": "^2.4.7",
    "@vinrobot/ubuntu-cloud-images": "^1.0.0",
    "@vinrobot/vmware-content-library": "^1.0.0",
    "node-fetch": "^2.6.11",
    "uuid": "^9.0.0",
    "vm2": "^3.9.19"
  },
  "devDependencies": {
    "@types/node": "^16.18.34",
    "@types/node-fetch": "^2.6.4",
    "@types/uuid": "^9.0.1",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4.0.0",
    "eslint-config-oclif-typescript": "^1.0.3",
    "oclif": "^3.9.0",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.2",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "scraper",
    "dirname": "scraper",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " "
  }
}
